<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MTurk_Calc.Styles">

    <Color x:Key="headerBackground">#B0C4DE</Color>
    <SolidColorBrush x:Key="headerBackgroundBrush" Color="{StaticResource headerBackground}"/>

    <Color x:Key="listViewAlternate">#D5E9FF</Color>
    <SolidColorBrush x:Key="listViewAlternateBrush" Color="{StaticResource listViewAlternate}"/>
</ResourceDictionary>